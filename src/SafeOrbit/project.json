{
  "copyright": "MIT-licensed",
  "language": "en",
  "description": "SafeOrbit is easy-to-use and strong security toolkit for .NET",
  "title": "SafeOrbit",
  "configurations": {
    "debug": {
      "buildOptions": {
        "define": ["DEBUG", "TRACE"]
      }
    },
    "release": {
      "buildOptions": {
        "define": ["RELEASE", "TRACE"],
        "optimize": true,
        /* assembly sign */
        "delaySign": true,
        "keyFile": "../../docs/signature_keys/private/SafeOrbit-key.snk",
        "strongName": true
      }
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Threading.Tasks.Parallel": "4.0.1",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Security.Cryptography.ProtectedData": "4.0.0",
        "System.Threading.Thread": "4.0.0",
        "System.Threading.ThreadPool": "4.0.10",
        "System.Security.Cryptography.Algorithms": "4.2.0",
        "System.Xml.ReaderWriter": "4.0.11",
        "System.Reflection.Metadata": "1.4.1-beta-24322-03",
        "System.ComponentModel": "4.0.1"
      },
      "buildOptions": {
        "define": ["NETCORE"]
      }
    },
    "net46": {
      "buildOptions": {
        "define": ["NET46"]
      },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0",
        "System.Security": "4.0.0"
      }
    }
  },
  "buildOptions": { "allowUnsafe": true },
  "version": "0.2.0-*"
}
